generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Song {
  id     String    @id @default(auto()) @map("_id") @db.ObjectId
  title  String
  artist String
  file   String
  duration Int // in milliseconds
  album  String
  albumImage String

}
